<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.twinkle.twinklechat.chatlist.ChatList
            android:id="@+id/chatviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="10dp"
            app:backgroundColor="@color/white"
            app:countbackgroundcolor="@android:color/holo_green_dark"
            app:counttextcolor="@color/white"
            app:countvisibility="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:messagetextcolor="@android:color/holo_blue_light"
            app:timetextcolor="@android:color/holo_blue_dark"
            app:timevisiblity="true"
            app:titletextcolor="@android:color/holo_blue_dark"/>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>